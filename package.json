{
  "name": "volcanion-auth-nets",
  "version": "1.0.0",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "nodemon src/app.ts",
    "setup:db": "npx ts-node database/setup.ts",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:prod": "docker-compose --env-file .env.production up -d",
    "docker:build": "docker-compose build --no-cache",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker-compose down -v --rmi all",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.{js,ts} --fix",
    "lint:check": "eslint src/**/*.{js,ts}",
    "format": "prettier --write src/**/*.{js,ts,json}",
    "format:check": "prettier --check src/**/*.{js,ts,json}",
    "type-check": "tsc --noEmit",
    "performance:test": "artillery run tests/performance/auth-load-test.yml",
    "security:audit": "npm audit --audit-level moderate",
    "prepare": "husky install"
  },
  "keywords": [
    "authentication",
    "authorization",
    "jwt",
    "express",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "description": "Authentication and Authorization system with JWT, MySQL, and Redis",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "joi": "^18.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "mysql2": "^3.14.3",
    "nodemailer": "^7.0.5",
    "redis": "^5.8.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.2.0",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "artillery": "^2.0.23",
    "better-npm-audit": "^3.11.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "lint-staged": "^16.1.4",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
